  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch with mono",
      "type": "mono",
      "request": "launch",
      "program": "${workspaceRoot}/bin/Debug/app8.exe",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": "build"
